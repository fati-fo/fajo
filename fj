def input1():
    list1=[]
    for i in range (10):
        print("list1[" ,i, "]=")
        list1.append(float(input(" " )))
    user=int(input(" enter number ="))
    counter=0
    for i in range(10):
        if list1[i]==user:
            counter=counter+1
    print(" counter :",counter)
input1()
